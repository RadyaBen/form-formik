{
	"extends": [
		"react-app",
		"react-app/jest"
	],
	"rules": {
		"no-console": "off",
		"prefer-const": "error",
		"quotes": ["error", "single"],
		"jsx-quotes": ["error", "prefer-single"],
		"max-len": ["error", 120],
		"comma-dangle": ["error", "always-multiline"],
		"semi": ["warn", "always"],
		"import/order": ["error", {
			"groups": ["builtin", "external", "internal", "parent", "sibling", "index", "object", "type"],
			"newlines-between": "always-and-inside-groups"
		}]
	}
}